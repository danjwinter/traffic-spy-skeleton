<div class="container">
  <h3>Events Index</h3>

  <div class="row">
    <div>
      <div id="event_frequency">
        <h4>Event Popularity Contest</h4>
        <% @user.payloads.event_frequency.each do |k, v| %>
          <h5><a id='event_data_<%= k %>' href='/sources/<%= @user.identifier %>/events/<%= k.split('/')[-1] %>'> <%= k %></a>: <%= v %></h5>
        <% end %>
      </div>
    </div>
  </div>
</div>
